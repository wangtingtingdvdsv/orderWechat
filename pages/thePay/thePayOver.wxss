.container{
  display: flex;
  padding: 20rpx;
  flex-direction: column;
  justify-content:flex-start;
}
.paythings{
  display: flex;
  flex-direction: column;
  flex-wrap: wrap;
}
.paymoney{
  display: flex;
  flex-direction: row;
  width: 750rpx;
  flex-wrap: wrap;
  justify-content:space-between;
  border-bottom: 1rpx solid #E5E5E7;
}
.paypeople{
  margin-top: 40rpx;
  display: flex;
  flex-direction: row;
  width: 750rpx;
  flex-wrap: wrap;
  justify-content:space-between;
  border-bottom: 1rpx solid #E5E5E7;
}
.payaddress{
  display: flex;
  flex-direction: row;
  width: 750rpx;
  flex-wrap: wrap;
  justify-content:space-between;
  border-bottom: 1rpx solid #E5E5E7;
}
.paytime{
  display: flex;
